<%- include('../partials/header') %>

<table id="list">
  <thead>
    <tr>
      <th id="Title">devFeed</th>
      <th>Your Feed</th>
      <th>Comments</th>
    </tr>
  </thead>
  <tbody>
    <% posts.forEach(function(p) { %>
      <tr>
        <td><%= p.title %></td>
      </tr>
      <td><%= p.content %></td>
      <td></td>
      <td><a href="/posts/<%= p._id %>">Reply</a></td>
      <br>
    <% }); %>
  </tbody>
</table>

<%- include('../partials/footer') %>
