<%- include('../partials/header') %>

<table id="list">
  <thead>
    <tr>
      <!-- <th>See What's New!</th> -->
      <th id="devFeed">devFeed</th>
      <!-- <th>Comments!</th> -->
    </tr>
  </thead>
  <tbody>
    <% posts.forEach(function(p) { %>
      <tr>
        <td><%= p.title %></td>
      </tr>
      <td><%= p.content %></td>
      <td></td>
      <td><a href="/posts/<%= p._id %>">Reply</a></td>
      <br>
    <% }); %>
  </tbody>
</table>

<%- include('../partials/footer') %>
