<%- include('../partials/header') %>

<table id="list">
  <thead>
    <tr>
      <th>Title</th>
      <!-- <th>Release<br>Year</th> -->
      <th></th>
    </tr>
  </thead>
  <tbody>
    <% posts.forEach(function(p) { %>
      <tr>
        <td><%= p.title %></td>
        <td><%= p.releaseYear %></td>
        <td><a href="/posts/<%= p._id %>">Reply</a></td>
      </tr>
      <% }); %>
    </tbody>
  </table>
  
<%- include('../partials/footer') %>